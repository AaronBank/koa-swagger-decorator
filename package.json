{
  "name": "koa-swagger-decorator",
  "version": "1.6.4",
  "description": "using decorator to automatically generate swagger doc for koa-router",
  "main": "dist/index.js",
  "dependencies": {
    "@types/globby": "^8.0.0",
    "@types/koa-router": "*",
    "@types/ramda": "^0.25.36",
    "globby": "^8.0.1",
    "is-type-of": "^1.2.0",
    "koa-router": "*",
    "ramda": "^0.25.0"
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "example/**/*.js",
      "test/**/*.js"
    ]
  },
  "devDependencies": {
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/node": "^10.7.0",
    "concurrently": "^3.6.1",
    "husky": "^4.2.3",
    "intelli-espower-loader": "^1.0.1",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-cors": "^0.0.16",
    "koa-multer": "^1.0.2",
    "koa-static": "^4.0.3",
    "lodash": "^4.17.10",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "pre-commit": "^1.2.2",
    "prettier-eslint-cli": "^4.7.1",
    "sha1": "^1.1.1",
    "standard-version": "^7.1.0",
    "supertest": "^2.0.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.8.0",
    "url": "^0.11.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start:example": "ts-node -P example/tsconfig.json example/main",
    "start": "nodemon --watch example --exec npm run start:example",
    "prepublish": "npm run build",
    "next": "npm publish --dist-tag next",
    "cov": "nyc --reporter=lcov npm run test"
  },
  "keywords": [
    "decorator",
    "swagger",
    "koa",
    "koa-router"
  ],
  "author": "cody",
  "license": "MIT",
  "repository": "https://github.com/Cody2333/koa-swagger-decorator"
}
